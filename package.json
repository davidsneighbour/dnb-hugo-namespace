{
  "name": "@davidsneighbour/namespace",
  "description": "Some details about dnb-org elements.",
  "version": "1.0.9",
  "bugs": {
    "url": "https://github.com/davidsneighbour/namespace/issues"
  },
  "devDependencies": {
    "@davidsneighbour/commitlint-config": "^3.4.4",
    "@davidsneighbour/remark-config": "^3.4.4",
    "@davidsneighbour/standard-version-config": "^3.4.3",
    "@davidsneighbour/textlint-config": "^3.4.5",
    "@davidsneighbour/tools": "^5.5.5"
  },
  "homepage": "https://github.com/davidsneighbour/namespace#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidsneighbour/namespace.git"
  },
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "remark": "remark .",
    "textlint": "textlint .",
    "textlint:fix": "textlint --fix ."
  }
}
